package exceptions;

public class Runner {

	public static void main(String[] args) {
		Maths one = new Maths();

		try {
			one.doMeme(9, 10);
		} catch(TwentyOneException toe) {
			toe.showException();
		}
		
		try {
			one.doDivision(50, 0);
		} catch(DumbException deZero) {
			deZero.exceptionNoZero();
		}
	}
}
